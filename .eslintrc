{
    "parser": "babel-eslint",
    "rules": {
        "comma-dangle": 0,
        "curly": 2,
        "indent": [2, 2],
        "linebreak-style": [2, "unix"],
        "no-console": 0,
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "no-var": 2,
        "prefer-const": 1,
        "quotes": [2, "double"],
        "semi": [2, "always"]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "root": true
}
