{
    "parser": "babel-eslint",
    "rules": {
        "indent": [2, 2],
        "quotes": [2, "double"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "no-console": [1]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended"
}