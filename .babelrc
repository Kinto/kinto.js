{
  "plugins": [
    "transform-object-rest-spread", 
    "transform-async-to-generator", 
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "development": {
      "presets": ["es2015"]
    },
    "firefox": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
