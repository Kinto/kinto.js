{
    "compilerOptions": {
        "allowJs": true,
        "allowUnreachableCode": false,
        "lib":["dom","es6","dom","es7","scripthost"],
        "module": "commonjs",
        "target": "ES6",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": false,
        "noImplicitReturns": false,
        "noImplicitThis": false,
        "preserveConstEnums": true,
        "removeComments": false,
        "sourceMap": true,
        "strictNullChecks": true
    },
    "include": [
        "./src/**/*"
    ]
}
