{
  "source": "./src",
  "destination": "./esdoc",
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {"from": "^src", "to": "lib"}
        ]
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin", 
      "option": {"all": true}
    },
    {
      "name": "esdoc-integrate-test-plugin",
      "option": {
        "source": "./test"
      }
    }
  ]
}
