{
  "source": "./src",
  "destination": "./esdoc",
  "test": {
    "type": "mocha",
    "source": "./test"
  },
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {"from": "^src", "to": "lib"}
        ]
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin", 
      "option": {"all": true}
    }
  ]
}
